{
  "name": "teamtrack-desktop",
  "version": "1.0.0",
  "main": "dist/electron/main.js",
  "type": "commonjs",
  "scripts": {
    "start": "concurrently \"npm:serve:angular\" \"npm:serve:electron\"",
    "serve:angular": "cd src/app && ng serve --port 4200",
    "serve:electron": "wait-on http://localhost:4200 && electron .",
    "build:angular": "cd src/app && ng build",
    "postinstall": "cd src/app && npm install",
    "build:electron": "tsc -p tsconfig.electron.json",
    "electron:start": "npm run build:electron && electron dist/electron/main.js",
    "electron:dev": "tsc -p tsconfig.electron.json --watch & electron dist/electron/main.js"
  },
  "build": {
    "asar": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/ws": "^8.18.1",
    "7zip-bin": "^5.2.0",
    "electron": "^38.2.2",
    "electron-builder": "^26.0.12",
    "wait-on": "^8.0.5"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "active-win": "^8.2.1",
    "axios": "^1.12.2",
    "better-sqlite3": "^11.10.0",
    "body-parser": "^2.2.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "electron-store": "^8.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "libsodium-wrappers": "^0.7.15",
    "node-forge": "^1.3.1",
    "tweetnacl": "^1.0.3",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  }
}
